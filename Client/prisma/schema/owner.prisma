model Owner {
  owner_id        String    @unique
  name            String?
  phone_number    String?
  phone_type      String?
  phone_primary   Boolean?
  email           String
  email_type      String?
  email_primary   Boolean?
  street          String?
  region          String?
  address         String?
  city            String?
  state           String?
  postal_code     String?
  country         String?
  address_primary Boolean?
  accountId       String
  timestamp       DateTime? @default(now())
}